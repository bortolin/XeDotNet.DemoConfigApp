{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "SmtpServer": {
    "Hostname": "ServerName22",
    "Username": "Username",
    "Password": "MyPassword",
    "Port": 25
  },

  "FeatureManagement": {
    "EnableFTemp": true,
    "HeaderColorGray": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "EnableUmidity": {
      "EnabledFor": [
        {
          "Name": "Microsoft.TimeWindow",
          "Parameters": {
            "Start": "21 May 2023 01:00:00 +00:00",
            "End": "22 May 2023 01:00:00 +00:00"
          }
        }
      ]
    },
    "EnableRequestId": {
      "EnabledFor": [
        {
          "Name": "Claims",
          "Parameters": {
            "RequiredClaims": "Internal"
          }
        }
      ]
    }
  }
}